div>\n  );\n}\n\n(CompoundQuestion as any).whyDidYouRender = true;\n\nexport default CompoundQuestion;\n","/home/rodrigo/github/duopettaja/src/components/CompoundQuestion/styled.ts",[],"/home/rodrigo/github/duopettaja/src/redux/constants/index.ts",[],{"ruleId":"102","replacedBy":"103"},{"ruleId":"104","replacedBy":"105"},{"ruleId":"106","severity":1,"message":"107","line":8,"column":11,"nodeType":"108","messageId":"109","endLine":8,"endColumn":17},{"ruleId":"110","severity":1,"message":"111","line":12,"column":1,"nodeType":"112","endLine":16,"endColumn":3},{"ruleId":"110","severity":1,"message":"113","line":5,"column":1,"nodeType":"112","endLine":11,"endColumn":2},{"ruleId":"106","severity":1,"message":"114","line":9,"column":11,"nodeType":"108","messageId":"109","endLine":9,"endColumn":17},{"ruleId":"106","severity":1,"message":"107","line":15,"column":11,"nodeType":"108","messageId":"109","endLine":15,"endColumn":17},{"ruleId":"110","severity":1,"message":"111","line":10,"column":1,"nodeType":"112","endLine":12,"endColumn":2},{"ruleId":"106","severity":1,"message":"107","line":11,"column":11,"nodeType":"108","messageId":"109","endLine":11,"endColumn":17},"no-native-reassign",["115"],"no-negated-in-lhs",["116"],"@typescript-eslint/no-unused-vars","'IState' is defined but never used.","Identifier","unusedVar","import/no-anonymous-default-export","Assign object to a variable before exporting as module default","ExportDefaultDeclaration","Assign arrow function to a variable before exporting as module default","'IProps' is defined but never used.","no-global-assign","no-unsafe-negation"]